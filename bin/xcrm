#!/usr/bin/env python

"""
Removes files in xcode's intermediate build folder
"""

import re
import os

print('Paste it here:')

text = ""

for _ in range(5):
    text += raw_input()

matched_filepath = re.findall(r"'(.*?Build/Intermediates.*?)'", text)[0]

os.remove(matched_filepath)

print("rm {}".format(matched_filepath))
